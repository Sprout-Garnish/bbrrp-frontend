{
  "extends": "@react-native-community",
  "rules": {
    "quotes": ["error", "double"],
    "no-extend-native": "off",
    "no-alert": "off"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "ignorePatterns": ["modules/client/interfaces/**"]
}
